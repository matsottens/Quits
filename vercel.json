{
  "version": 2,
  "name": "quits-frontend",
  "buildCommand": "node vercel-build.js",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.quits.cc/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
